{
  "name": "boilerplate-parcel-react",
  "version": "1.0.0",
  "description": "boilerplate for react apps using parcel",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "default": {
      "distDir": "./public"
    }
  },
  "scripts": {
    "start": "AUTH0_DOMAIN=$npm_config_domain AUTH0_CLIENT_ID=$npm_config_client AUTH0_AUDIENCE=$npm_config_audience parcel --open --port 3030 --dist-dir ./public",
    "build": "parcel build",
    "clear": "rm -rf ./public .parcel-cache"
  },
  "keywords": [
    "parcel"
  ],
  "author": "dan moneypenny",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-react": "^1.9.0",
    "@parcel/core": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "parcel": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  }
}
